<script src="//maps.google.com/maps/api/js?v=3.18&sensor=false&client=&key=&libraries=geometry&language=&hl=&region="></script> 
<script src="//google-maps-utility-library-v3.googlecode.com/svn/tags/markerclustererplus/2.0.14/src/markerclusterer_packed.js"></script>
<script src='//google-maps-utility-library-v3.googlecode.com/svn/tags/infobox/1.1.9/src/infobox_packed.js' type='text/javascript'></script> <!-- only if you need custom infoboxes -->


<form class="form-style-9">
<div>
<p id="notice"><%= notice %></p>

<ul>
  <div class="field-style field-split align-left" type="text" name="field1"  placeholder="Name" >
  
  <strong>Address:</strong>
  <%= @location.address ? @location.address : " "%>

  </div>
  <div class="field-style" name="field5" >
   
  <strong>Latitude:</strong>
  <%= @location.latitude ? @location.latitude : "No Latitude"%>
  </div>
   <div class="field-style" name="field6" >
   
  <strong>Latitude:</strong>
  <strong>Longitude:</strong>
  <%= @location.longitude ? @location.longitude : "No Longitude"%>
  </div>
 
  </ul>


<div style='width: 800px;'>
  
 <%= image_tag("https://maps.googleapis.com/maps/api/staticmap?center=#{@location.address}&zoom=13&size=600x300&maptype=roadmap
&markers=color:blue%7Clabel:S%7C#{@location.latitude},#{@location.longitude}

&key=AIzaSyDJvPRbTHpEn-KfeKLlOuSPvX_cjDCDWxg") %>
</div>
<%= link_to 'Edit', edit_location_path(@location) %> |
<%= link_to 'Back', locations_path %>
</div>
  </form>